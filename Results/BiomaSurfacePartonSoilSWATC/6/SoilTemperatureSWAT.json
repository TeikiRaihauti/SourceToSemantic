{
    "metadata": {
        "Title": "SoilTemperatureSWAT",
        "Authors": "Simone Bregaglio (simone.bregaglio@unimi.it); Nicolò Frasso (nicolo.frasso@unimi.it)",
        "Institution": "University Of Milan",
        "URI": "http://bioma.jrc.ec.europa.eu/ontology/JRC_MARS_biophysical_domain.owl",
        "DOI": "-",
        "Extended description": "Strategy for the calculation of soil temperature with SWAT method. Reference: Neitsch,S.L., Arnold, J.G., Kiniry, J.R., Williams, J.R., King, K.W. Soil and Water Assessment Tool. Theoretical documentation. Version 2000. http://swatmodel.tamu.edu/media/1290/swat2000theory.pdf",
        "Short description": "None",
        "Model version": "001"
    },
    "init": {
        "name": "Init",
        "firstLine": "states.SoilTemperatureByLayers = new double[states.LayerThickness.Length];",
        "lastLine": "states.SoilTemperatureByLayers[i] = 15;"
    },
    "process": {
        "name": "Estimate",
        "description": "Strategy for the calculation of soil temperature with SWAT method.",
        "firstLine": "double _SoilProfileDepthmm = states.SoilProfileDepth * 1000;",
        "lastLine": "states.SoilTemperatureByLayers[i] = LagCoefficient * states.SoilTemperatureByLayers[i] +\n                                                  (1 - LagCoefficient) *\n                                                  (_DepthFactor *\n                                                   (exogenous.AirTemperatureAnnualAverage - states.SurfaceSoilTemperature) +\n                                                   states.SurfaceSoilTemperature);"
    },
    "inputs": [
        {
            "name": "states_SoilProfileDepth",
            "description": "Soil profile depth",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0",
            "max": "50",
            "default": "3",
            "unit": "m",
            "uri": "http://"
        },
        {
            "name": "states_LayerThickness",
            "description": "Soil layer thickness",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLEARRAY",
            "len": "-",
            "min": "0.005",
            "max": "3",
            "default": "0.05",
            "unit": "m",
            "uri": "http://"
        },
        {
            "name": "states_VolumetricWaterContent",
            "description": "Volumetric soil water content",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLEARRAY",
            "len": "-",
            "min": "0",
            "max": "0.8",
            "default": "0.25",
            "unit": "m3 m-3",
            "uri": "http://"
        },
        {
            "name": "states_BulkDensity",
            "description": "Bulk density",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLEARRAY",
            "len": "-",
            "min": "0.9",
            "max": "1.8",
            "default": "1.3",
            "unit": "t m-3",
            "uri": "http://"
        },
        {
            "name": "states_SoilTemperatureByLayers",
            "description": "Soil temperature of each layer",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLEARRAY",
            "len": "-",
            "min": "-60",
            "max": "60",
            "default": "15",
            "unit": "°C",
            "uri": "http://"
        },
        {
            "name": "states_SurfaceSoilTemperature",
            "description": "Average surface soil temperature",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-60",
            "max": "60",
            "default": "25",
            "unit": "°C",
            "uri": "http://"
        },
        {
            "name": "exogenous_AirTemperatureAnnualAverage",
            "description": "Annual average air temperature",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-40",
            "max": "50",
            "default": "15",
            "unit": "°C",
            "uri": "http://"
        },
        {
            "name": "parameter_LagCoefficient",
            "description": "Lag coefficient that controls the influence of the previous day's temperature on the current day's temperature",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0",
            "max": "1",
            "default": "0.8",
            "unit": "dimensionless",
            "uri": ""
        }
    ],
    "outputs": [
        {
            "name": "states_SoilTemperatureByLayers",
            "description": "Soil temperature of each layer",
            "datatype": "DOUBLEARRAY",
            "category": "state",
            "len": "-",
            "min": "-60",
            "max": "60",
            "default": "15",
            "unit": "°C",
            "uri": "http://"
        }
    ],
    "functions": [
        {
            "name": "Math.Exp",
            "description": "Returns e raised to the specified power (exponential function)."
        },
        {
            "name": "Math.Log",
            "description": "Returns the natural (base e) logarithm of a specified number."
        },
        {
            "name": "Math.Pow",
            "description": "Returns a specified number raised to the specified power."
        }
    ],
    "tests": "-"
}
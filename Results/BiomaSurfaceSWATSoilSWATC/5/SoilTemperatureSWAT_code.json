        },
        {
            "name": "Estimate",
            "description": "Update SoilTemperatureByLayers using SWAT method."
        },
        {
            "name": "_compute_maximum_dumping_depth",
            "description": "Compute maximum dumping depth (mm): 1000 + (2500*BD)/(BD + 686*exp(-5.63*BD))."
        },
        {
            "name": "_compute_scaling_factor",
            "description": "Compute scaling factor SF = TWCmm / ((0.356 - 0.144*BD) * SPDmm)."
        },
        {
            "name": "_compute_dumping_depth",
            "description": "Compute dumping depth DD = MDD * exp(ln(500/MDD) * ((1 - SF)/(1 + SF))^2)."
        },
        {
            "name": "_compute_depth_factor",
            "description": "Compute depth factor DF = RC / (RC + exp(-0.867 - 2.078*RC))."
        }
    ],

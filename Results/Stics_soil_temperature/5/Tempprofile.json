{
    "metadata": {
        "Title": "tempprofile model",
        "Authors": "STICS",
        "Institution": "INRAE",
        "URI": "",
        "DOI": "http://dx.doi.org/10.1016/j.agrformet.2014.05.002",
        "Extended description": "Calculates soil temperature profile",
        "Short description": "Calculates soil temperature profile",
        "Model version": "1.0"
    },
    "init": {
        "name": "init_temp_profile",
        "firstLine": "integer :: soil_depth",
        "lastLine": "prev_canopy_temp = air_temp_day1"
    },
    "process": {
        "name": "model_temp_profile",
        "description": "Calculates current soil profile temperature (for 1 cm layers)",
        "firstLine": "n = size(prev_temp_profile)",
        "lastLine": "END DO"
    },
    "inputs": [
        {
            "name": "temp_amp",
            "description": "current temperature amplitude",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0.0",
            "max": "100.0",
            "default": "0.0",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "prev_temp_profile",
            "description": "previous soil temperature profile (for 1 cm layers)",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLEARRAY",
            "len": "-",
            "min": "-50.0",
            "max": "50.0",
            "default": "-",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "prev_canopy_temp",
            "description": "previous crop temperature",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0.0",
            "max": "50.0",
            "default": "-",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "min_air_temp",
            "description": "current minimum air temperature",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-50.0",
            "max": "50.0",
            "default": "-",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "air_temp_day1",
            "description": "Mean temperature on first day",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0.0",
            "max": "100.0",
            "default": "0.0",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "layer_thick",
            "description": "layers thickness",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "INTARRAY",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "cm",
            "uri": ""
        },
        {
            "name": "therm_diff",
            "description": "thermal diffusivity constant used to compute thermal amplitude",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "5.37e-3",
            "unit": "-",
            "uri": ""
        },
        {
            "name": "temp_freq",
            "description": "temperature frequency constant used to compute thermal amplitude",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "7.272e-5",
            "unit": "-",
            "uri": ""
        }
    ],
    "outputs": [
        {
            "name": "temp_profile",
            "description": "current soil profile temperature (for 1 cm layers)",
            "datatype": "DOUBLEARRAY",
            "category": "state",
            "len": "-",
            "min": "-50.0",
            "max": "50.0",
            "default": "-",
            "unit": "degC",
            "uri": ""
        }
    ],
    "functions": [
        {
            "name": "-",
            "description": "-"
        }
    ],
    "tests": [
        {
            "name": "-",
            "description": "-",
            "inputs": [
                {
                    "name": "-",
                    "value": "-"
                }
            ],
            "outputs": [
                {
                    "name": "-",
                    "value": "-"
                }
            ]
        }
    ]
}
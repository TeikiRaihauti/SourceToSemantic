    "outputs": [
        {
            "name": "SoilTempArray",
            "description": "Updated soil temperature per layer after the daily step",
            "datatype": "DOUBLELIST",
            "category": "state",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "rSoilTempArrayRate",

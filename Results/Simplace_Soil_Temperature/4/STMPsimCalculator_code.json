            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "cABD",
            "description": "Mean bulk density",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "t/m3",
            "uri": "-"
        },
        {
            "name": "iSoilWaterContent",
            "description": "Water content, sum over whole soil profile",

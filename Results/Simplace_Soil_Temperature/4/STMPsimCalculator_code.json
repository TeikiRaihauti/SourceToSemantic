            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0.0",
            "max": "-",
            "default": "-",
            "unit": "mm",
            "uri": "-"
        },
        {
            "name": "iSoilSurfaceTemperature",
            "description": "Soil surface temperature",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "iDoInitialize",
            "description": "If True, reinitialize the model state before processing the day",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "INT",
            "len": "-",
            "min": "0",
            "max": "1",
            "default": "-",
            "unit": "-",
            "uri": "-"
        },
        {
            "name": "SoilTempArray",
            "description": "Current soil temperature per layer",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "rSoilTempArrayRate",
            "description": "Current daily temperature change per layer",
            "inputtype": "variable",
            "category": "rate",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC/day",
            "uri": "-"
        },
        {
            "name": "pSoilLayerDepth",
            "description": "Depths to the bottom of each (possibly extended) soil layer",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "0.0",
            "max": "-",
            "default": "-",
            "unit": "m",
            "uri": "-"
        }

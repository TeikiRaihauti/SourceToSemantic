        "description": "Main daily soil temperature process for the STMP model."
    },
    "inputs": [
        {
            "name": "cSoilLayerDepth",
            "description": "Depths to the bottom of each soil layer",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "0.0",
            "max": "-",
            "default": "-",
            "unit": "m",
            "uri": "-"
        },
        {
            "name": "cFirstDayMeanTemp",
            "description": "Mean air temperature on the first day",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "cDampingDepth",
            "description": "Initial damping depth of soil",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0.0",
            "max": "-",
            "default": "-",
            "unit": "m",
            "uri": "-"
        },
        {
            "name": "cAVT",
            "description": "Constant temperature of deepest soil layer - long term mean air temperature",

{
    "init": {
        "name": "stmp_initialize"
    },
    "process": {
        "name": "stmp_process",
        "description": "Advance soil temperatures one day using the APEX soil temperature approach (Williams & Izaurralde, 2005)."
    },
    "inputs": [
        {
            "name": "cSoilLayerDepth",
            "description": "List of soil layer bottom depths, increasing, profile bottom at last element",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "m",
            "uri": ""
        },
        {
            "name": "cFirstDayMeanTemp",
            "description": "Mean air temperature on first day for initialization/re-initialization",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "cDampingDepth",
            "description": "Damping depth used to extend the profile and initialize temperatures",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "m",
            "uri": ""
        },
        {
            "name": "cAVT",
            "description": "Long-term average annual air temperature",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "cABD",
            "description": "Average soil bulk density",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "t m-3",
            "uri": ""
        },
        {
            "name": "iSoilWaterContent",
            "description": "Total water stored in soil profile",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "mm",
            "uri": ""
        },
        {
            "name": "iSoilSurfaceTemperature",
            "description": "Current soil surface temperature",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "SoilTempArray",
            "description": "Current soil temperatures for each (extended) layer before update",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "pSoilLayerDepth",
            "description": "Extended layer bottom depths including layers to reach damping depth",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "m",
            "uri": ""
        },
        {
            "name": "iDoInitialize",
            "description": "Flag to re-initialize states before processing this day (0/1)",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "INT",
            "len": "-",
            "min": "0",
            "max": "1",
            "default": "",
            "unit": "-",
            "uri": ""
        }
    ],
    "outputs": [
        {
            "name": "SoilTempArray_new",
            "description": "Updated soil temperatures for each (extended) layer after one day",
            "datatype": "DOUBLELIST",
            "category": "state",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "degC",
            "uri": ""
        },
        {
            "name": "rSoilTempArrayRate_new",
            "description": "Daily soil temperature change rate for each (extended) layer",
            "datatype": "DOUBLELIST",
            "category": "state",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "degC/day",
            "uri": ""
        },
        {
            "name": "pSoilLayerDepth_new",
            "description": "Extended layer bottom depths used for this step",
            "datatype": "DOUBLELIST",
            "category": "state",
            "len": "-",
            "min": "",
            "max": "",
            "default": "",
            "unit": "m",
            "uri": ""
        }
    ],
    "functions": [],
    "tests": [
        {
            "name": "test_stmp_process_case0",
            "description": "Derived from STMPsimCalculator.fillTestVariables test case (aTestIndex == 0).",
            "inputs": [
                {
                    "name": "cSoilLayerDepth",
                    "value": [
                        0.1,
                        0.5,
                        1.5
                    ]
                },
                {
                    "name": "cFirstDayMeanTemp",
                    "value": 15.0
                },
                {
                    "name": "cAVT",
                    "value": 9.0
                },
                {
                    "name": "cABD",
                    "value": 1.4
                },
                {
                    "name": "cDampingDepth",
                    "value": 6.0
                },
                {
                    "name": "iSoilWaterContent",
                    "value": 0.3
                },
                {
                    "name": "iSoilSurfaceTemperature",
                    "value": 6.0
                },
                {
                    "name": "iDoInitialize",
                    "value": false
                }
            ],
            "outputs": [
                {
                    "name": "SoilTempArray_new",
                    "value": [
                        13.624360856350041,
                        13.399968504634286,
                        12.599999999999845,
                        12.2,
                        11.4,
                        10.6,
                        9.799999999999999,
                        9.0
                    ]
                }
            ]
        }
    ]
}
{
    "init": {
        "name": "init"
    },
    "process": {
        "name": "process",
        "description": "Main biophysical process function for daily soil temperature update (STMPsimCalculator)."
    },
    "inputs": [
        {
            "name": "iDoInitialize",
            "description": "Boolean flag; if True, re-initializes state using init() before processing",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "INT",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "-",
            "uri": "-"
        },
        {
            "name": "cSoilLayerDepth",
            "description": "Depths to the bottom of each soil layer",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "m",
            "uri": "-"
        },
        {
            "name": "cFirstDayMeanTemp",
            "description": "Mean air temperature on first day (used for initialization)",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "cDampingDepth",
            "description": "Initial damping depth of soil (used for initialization)",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "m",
            "uri": "-"
        },
        {
            "name": "cABD",
            "description": "Mean bulk density",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "t m-3",
            "uri": "-"
        },
        {
            "name": "cAVT",
            "description": "Long-term average annual air temperature (deepest soil layer temperature)",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "iSoilWaterContent",
            "description": "Soil water stored in the profile",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "mm",
            "uri": "-"
        },
        {
            "name": "iSoilSurfaceTemperature",
            "description": "Soil surface temperature (DST0)",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "SoilTempArray",
            "description": "Current soil temperature per (possibly extended) layer",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "rSoilTempArrayRate",
            "description": "Current daily temperature change per layer",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC/day",
            "uri": "-"
        },
        {
            "name": "pSoilLayerDepth",
            "description": "Depth to bottom of each (possibly extended) soil layer",
            "inputtype": "variable",
            "category": "state",
            "datatype": "DOUBLELIST",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "m",
            "uri": "-"
        }
    ],
    "outputs": [
        {
            "name": "SoilTempArray_new",
            "description": "Updated soil temperatures per layer after one process step",
            "datatype": "DOUBLELIST",
            "category": "state",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": "-"
        },
        {
            "name": "rSoilTempArrayRate_new",
            "description": "Updated daily temperature change per layer after one process step",
            "datatype": "DOUBLELIST",
            "category": "state",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "degC/day",
            "uri": "-"
        },
        {
            "name": "pSoilLayerDepth_new",
            "description": "Possibly re-initialized depth to bottom of layers after one process step",
            "datatype": "DOUBLELIST",
            "category": "state",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "m",
            "uri": "-"
        }
    ],
    "functions": [],
    "tests": [
        {

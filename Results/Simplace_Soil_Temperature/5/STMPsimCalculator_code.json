            "inputs": [
                {
                    "name": "cSoilLayerDepth",
                    "value": [
                        0.1,
                        0.5,
                        1.5
                    ]
                },
                {
                    "name": "cFirstDayMeanTemp",
                    "value": 15.0
                },
                {
                    "name": "cAVT",
                    "value": 9.0
                },
                {
                    "name": "cABD",
                    "value": 1.4
                },
                {
                    "name": "cDampingDepth",
                    "value": 6.0
                },
                {
                    "name": "iSoilWaterContent",
                    "value": 0.3
                },
                {
                    "name": "iSoilSurfaceTemperature",
                    "value": 6.0
                },
                {
                    "name": "iDoInitialize",
                    "value": false
                }
            ],
            "outputs": [
                {
                    "name": "expected_SoilTempArray",
                    "value": [
                        13.624360856350041,
                        13.399968504634286,
                        12.599999999999845,
                        12.2,
                        11.4,
                        10.6,
                        9.799999999999999,
                        9.0
                    ]
                }
            ]
        }
    ]
}
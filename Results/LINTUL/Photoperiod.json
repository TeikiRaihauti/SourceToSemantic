{
    "init": {
        "name": "init"
    },
    "process": {
        "name": "process",
        "description": "Compute daily photoperiod factor from photoperiod thresholds and day length with integer hour conversion and clamping."
    },
    "inputs": [
        {
            "name": "iPhotoInhibition",
            "description": "Photoperiod inhibition threshold in hours.",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0",
            "max": "24",
            "default": "14",
            "unit": "hour",
            "uri": "http://www.wurvoc.org/vocabularies/om-1.8/hour"
        },
        {
            "name": "iPhotoInsensitivity",
            "description": "Photoperiod insensitivity threshold in hours.",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "0",
            "max": "24",
            "default": "6",
            "unit": "hour",
            "uri": "http://www.wurvoc.org/vocabularies/om-1.8/hour"
        },
        {
            "name": "iDayLength",
            "description": "Day length in minutes.",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "INT",
            "len": "-",
            "min": "0",
            "max": "1440",
            "default": "-",
            "unit": "minute",
            "uri": "http://www.wurvoc.org/vocabularies/om-1.8/minute"
        }
    ],
    "outputs": [
        {
            "name": "PhotoPeriodFactor",
            "description": "Daily photoperiod factor, clamped to [0,1].",
            "datatype": "DOUBLE",
            "category": "auxiliary",
            "len": "-",
            "min": "0",
            "max": "1",
            "default": "0",
            "unit": "one",
            "uri": "http://www.wurvoc.org/vocabularies/om-1.8/one"
        }
    ],
    "functions": [
        {
            "name": "-",
            "description": "-"
        }
    ],
    "tests": [
        {
            "name": "-",
            "description": "-",
            "inputs": [
                {
                    "name": "-",
                    "value": "-"
                }
            ],
            "outputs": [
                {
                    "name": "-",
                    "value": "-"
                }
            ]
        }
    ],
    "comments": [
        {
            "comment": "Minutes are converted to hours using integer division (iDayLength/60)."
        },
        {
            "comment": "PhotoPeriodFactor is an output (auxiliary), not a state input; it is clamped to [0,1]. If the denominator is zero and the numerator is zero, the result is NaN and remains unchanged by clamping."
        },
        {
            "comment": "Code comments for iPhotoInhibition and iPhotoInsensitivity in createVariables are mislabeled; descriptions here reflect their actual photoperiod roles."
        }
    ]
}
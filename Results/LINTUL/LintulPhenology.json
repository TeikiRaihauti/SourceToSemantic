            "category": "auxiliary",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "-",
            "uri": "http://www.wurvoc.org/vocabularies/om-1.8/one"
        },
        {
            "name": "SowingDOY",
            "description": "DOY of sowing",
            "datatype": "INT",
            "category": "auxiliary",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "0",
            "unit": "-",
            "uri": "http://www.wurvoc.org/vocabularies/om-1.8/one"
        },

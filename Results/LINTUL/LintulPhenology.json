            "datatype": "DOUBLE",
            "category": "auxiliary",
            "len": "-",
            "min": "0",
            "max": "-",
            "default": "0",
            "unit": "1/day",
            "uri": "http://www.wurvoc.org/vocabularies/om-1.8/reciprocal_day"

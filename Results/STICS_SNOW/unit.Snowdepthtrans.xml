<?xml version="1.0" ?>
<ModelUnit modelid=".SnowDepthTrans" name="SnowDepthTrans" timestep="1" version="1.0">
	<Description>
		<Title>SnowDepthTrans</Title>
		<Authors>STICS</Authors>
		<Institution>INRA</Institution>
		<URI>-</URI>
		<Reference>http://dx.doi.org/10.1016/j.agrformet.2014.05.002</Reference>
		<ExtendedDescription>STICS component computing snow cover depth in centimeters by multiplying the snow depth Sdepth (m) by a conversion/density factor Pns (cm per m). Implemented in Fortran90 as subroutine model_snowdepthtrans; intended time step is 1.</ExtendedDescription>
		<ShortDescription>Converts snow depth from meters to centimeters using Pns.</ShortDescription>
	</Description>
	<Inputs>
		<Input name="Sdepth" description="snow cover depth" inputtype="variable" variablecategory="state" datatype="DOUBLE" max="500.0" min="0.0" default="0.0" unit="m" uri="-"/>
		<Input name="Pns" description="density of the new snow" inputtype="parameter" parametercategory="constant" datatype="DOUBLE" max="-" min="-" default="100.0" unit="cm/m" uri="-"/>
	</Inputs>
	<Outputs>
		<Output name="Sdepth_cm" description="snow cover depth in cm" variablecategory="state" datatype="DOUBLE" max="500.0" min="0.0" unit="cm" uri="-"/>
	</Outputs>
	<Function name="-" description="-" language="cyml" type="external" filename="algo/pyx/-.pyx"/>
	<Algorithm language="cyml" platform="" filename="algo/pyx/model_snowdepthtrans.pyx"/>
	<Parametersets/>
	<Testsets>
		<Testset name="t_-" description="-">
			<Test name="t_-">
				<OutputValue name="-">-</OutputValue>
			</Test>
		</Testset>
	</Testsets>
</ModelUnit>

{
    "init": {
        "name": "-"
    },
    "process": {
        "name": "CalculateModel",
        "description": "Compute evapoTranspirationPenman using the Penman-Monteith formulation."
    },
    "inputs": [
        {
            "name": "evapoTranspirationPriestlyTaylor",
            "description": "Priestly-Taylor evapotranspiration",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "g m-2 d-1",
            "uri": "-"
        },
        {
            "name": "hslope",
            "description": "Slope of saturated vapor pressure curve at air temperature",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "hPa °C-1",
            "uri": "-"
        },
        {
            "name": "VPDair",
            "description": "Vapor Pressure Deficit of air",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "hPa",
            "uri": "-"
        },
        {
            "name": "conductance",
            "description": "Boundary layer conductance",
            "inputtype": "variable",
            "category": "exogenous",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "m d-1",
            "uri": "-"
        },
        {
            "name": "psychrometricConstant",
            "description": "Psychrometric constant",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "0.66",
            "unit": "hPa °C-1",
            "uri": "-"
        },
        {
            "name": "Alpha",
            "description": "Priestley-Taylor proportionality constant",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "1.5",
            "unit": "-",
            "uri": "-"
        },
        {
            "name": "lambdaV",
            "description": "Latent heat of vaporization of water",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "2.454",
            "unit": "MJ kg-1",
            "uri": "-"
        },
        {
            "name": "rhoDensityAir",
            "description": "Density of air",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "1.225",
            "unit": "kg m-3",
            "uri": "-"
        },
        {
            "name": "specificHeatCapacityAir",
            "description": "Specific heat capacity of dry air",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLE",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "0.00101",
            "unit": "MJ kg-1 °C-1",
            "uri": "-"
        }
    ],
    "outputs": [
        {
            "name": "evapoTranspirationPenman",
            "description": "Penman-Monteith evapotranspiration",
            "datatype": "DOUBLE",
            "category": "state",
            "len": "-",
            "min": "-",
            "max": "-",
            "default": "-",
            "unit": "g m-2 d-1",
            "uri": "-"
        }
    ],
    "functions": [
        {
            "name": "CalculateModel",
            "description": "Returns Penman-Monteith evapotranspiration as Priestly-Taylor term divided by Alpha plus an aerodynamic term based on air properties, VPD, conductance, and psychrometric terms."
        }
    ],
    "tests": "-",
    "comments": [
        {
            "comment": "No initialization or persistent state. Function may raise ZeroDivisionError if Alpha == 0 or if (hslope + psychrometricConstant) == 0. Output treated as 'state' to comply with schema."
        }
    ]

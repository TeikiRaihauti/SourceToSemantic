    "metadata": {
        "Title": "STEMP_EPIC",
        "Authors": "Kenneth N. Potter; Jimmy R. Williams",
        "Institution": "USDA-ARS; University of Georgia; University of Florida; Iowa State University; International Center for Soil Fertility and Agricultural Development; University of Guelph",
        "URI": "-",
        "DOI": "https://doi.org/10.2134/agronj1994.00021962008600060014x",
        "Extended description": "Determines soil temperature by layer using the EPIC soil temperature method (Potter & Williams). Daily timestep component that initializes and updates soil and surface temperatures and calls the SOILT_EPIC routine for layer calculations.",
        "Short description": "Determines soil temperature by layer",
        "Model version": "1.0"
    },
    "init": {
        "name": "STEMP_EPIC",
        "firstLine": "SWI = SW",
        "lastLine": "END DO"
    },
    "process": {
        "name": "STEMP_EPIC",
        "description": "Top-level subroutine that initializes and updates soil temperature by layer (daily). It prepares soil/profile variables and calls SOILT_EPIC to compute layer temperatures.",
        "firstLine": "ISWWAT = ISWITCH % ISWWAT",
        "lastLine": "END SUBROUTINE STEMP_EPIC"
    },

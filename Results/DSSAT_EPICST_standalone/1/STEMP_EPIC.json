{
    "metadata": {
        "Title": "STEMP_EPIC (EPIC soil temperature module)",
        "Authors": "Kenneth N. Potter (USDA-ARS); Jimmy R. Williams (USDA-ARS)",
        "Institution": "USDA-ARS; University of Georgia; University of Florida; Iowa State University; International Center of Soil Fertility and Agricultural Development; University of Guelph",
        "URI": "-",
        "DOI": "https://doi.org/10.2134/agronj1994.00021962008600060014x",
        "Extended description": "Determines soil temperature by layer using the EPIC method (Potter & Williams, 1994). STEMP_EPIC initializes soil state and calls SOILT_EPIC which computes daily soil temperatures for each soil layer.",
        "Short description": "Determines soil temperature by layer",
        "Model version": "1.0"
    },
    "init": {
        "name": "STEMP_EPIC",
        "firstLine": "SWI = SW",
        "lastLine": "END DO"
    },
    "process": {
        "name": "SOILT_EPIC",
        "description": "Computes soil temperature by soil layer using the EPIC method (Potter & Williams). Uses soil properties, moisture, weather and recent temperature memory to update layer temperatures and surface temperature.",
        "firstLine": "WC = AMAX1(0.01, PESW) / (WW * CUMDPT) * 10.0",
        "lastLine": "X2_PREV = X2_AVG"
    },
    "inputs": [
        {
            "name": "SOILPROP_BD",
            "description": "Bulk density, soil layer (g soil / cm3 soil)",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLEARRAY",
            "len": "NL",
            "min": "-",
            "max": "-",
            "default": "1.6",
            "unit": "g/cm3",
            "uri": "-"
        },
        {
            "name": "SOILPROP_DLAYR",
            "description": "Thickness of soil layer (cm)",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLEARRAY",
            "len": "NL",
            "min": "-",
            "max": "-",
            "default": "10.0",
            "unit": "cm",
            "uri": "-"
        },
        {
            "name": "SOILPROP_DS",
            "description": "Cumulative depth in soil layer (cm)",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLEARRAY",
            "len": "NL",
            "min": "-",
            "max": "-",
            "default": "[10.0,20.0,30.0,40.0]",
            "unit": "cm",
            "uri": "-"
        },
        {

    "metadata": {
        "Title": "STEMP_EPIC",
        "Authors": "Kenneth N. Potter; Jimmy R. Williams",
        "Institution": "USDA-ARS",
        "URI": "-",
        "DOI": "https://doi.org/10.2134/agronj1994.00021962008600060014x",
        "Extended description": "Determines soil temperature by soil layer using the EPIC soil temperature method (Potter & Williams, 1994). Daily timestep implementation. Initializes soil thermal state and computes daily soil temperatures by layer.",
        "Short description": "Determines soil temperature by layer",
        "Model version": "1.0"
    },
    "init": {
        "name": "STEMP_EPIC (seasonal initialization section)",
        "firstLine": "SWI = SW",
        "lastLine": "END DO"
    },
    "process": {
        "name": "SOILT_EPIC",
        "description": "Calculates soil temperature by layer using EPIC method (Potter & Williams, 1994). Computes surface temperature and updates layer temperatures based on water content, cover and thermal damping depth.",
        "firstLine": "WC = AMAX1(0.01, PESW) / (WW * CUMDPT) * 10.0",
        "lastLine": "X2_PREV = X2_AVG"
    },

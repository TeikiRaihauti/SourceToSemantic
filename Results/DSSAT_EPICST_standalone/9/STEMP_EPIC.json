{
    "metadata": {
        "Title": "STEMP_EPIC (EPIC soil temperature)",
        "Authors": "Kenneth N. Potter; Jimmy R. Williams",
        "Institution": "USDA-ARS (authors); Copyright holders include University of Georgia; University of Florida; Iowa State University; International Center for Soil Fertility and Agricultural Development; University of Guelph",
        "URI": "",
        "DOI": "https://doi.org/10.2134/agronj1994.00021962008600060014x",
        "Extended description": "Determines soil temperature by layer using the EPIC soil temperature method. Computes daily soil temperature profile and surface litter temperature based on soil properties, water content, and weather.",
        "Short description": "Determines soil temperature by layer (EPIC method).",
        "Model version": "1.0"
    },
    "init": {
        "name": "STEMP_EPIC",
        "firstLine": "SWI = SW",
        "lastLine": "END DO"
    },
    "process": {
        "name": "SOILT_EPIC",
        "description": "Core routine that computes soil temperature by layer (ST) and surface temperature (SRFTEMP) using EPIC equations and recent temperature history (TMA).",
        "firstLine": "WC = AMAX1(0.01, PESW) / (WW * CUMDPT) * 10.0",
        "lastLine": "X2_PREV = X2_AVG"
    },

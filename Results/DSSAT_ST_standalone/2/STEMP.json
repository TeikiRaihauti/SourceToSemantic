        {
            "name": "SRFTEMP",
            "description": "Temperature of soil surface litter (surface temperature)",
            "datatype": "DOUBLE",
            "category": "state",
            "len": "-",
            "min": "-273.15",
            "max": "-",
            "default": "-",
            "unit": "degC",
            "uri": ""
        }

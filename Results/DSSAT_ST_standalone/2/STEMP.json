            "min": "0.0",
            "max": "-",
            "default": "-",
            "unit": "cm",
            "uri": ""
        },
        {
            "name": "SOILPROP_DUL",
            "description": "Volumetric soil water content at Drained Upper Limit in layer L",
            "inputtype": "parameter",
            "category": "constant",
            "datatype": "DOUBLEARRAY",
            "len": "NL",
            "min": "0.0",

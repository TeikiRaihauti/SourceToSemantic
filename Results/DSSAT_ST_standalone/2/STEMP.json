    "functions": [
        {
            "name": "STEMP",
            "description": "Main subroutine wrapper: handles initialization (seasonal), daily rate processing and calls SOILT to compute soil temperatures; prepares inputs and transfers soil property fields."
        },
        {
            "name": "SOILT",
            "description": "Core soil temperature calculation: computes damping depth and layer temperatures; updates TMA/ATOT and returns ST and SRFTEMP."
        },
        {
            "name": "OPSTEMP",
            "description": "Output routine that writes soil temperature results to SoilTemp.OUT (file output formatting)."
        },
        {
            "name": "YR_DOY",
            "description": "Utility routine converting YRDOY (YYYYDDD) to YR and DOY."
        },
        {
            "name": "ASKEE",
            "description": "Standalone program (driver) used to exercise/test STEMP: constructs CONTROL, ISWITCH and SOILPROP, sets weather and calls STEMP."
        },
        {
            "name": "ModuleDefs",
            "description": "Module defining constants and derived types (e.g., NL, ControlType, SwitchType, SoilType) used across routines."
        }
    ],

{
    "metadata": {
        "Title": "STEMP",
        "Authors": "DSSAT (DSSAT Florida)",
        "Institution": "University of Georgia; University of Florida; Iowa State University; International Center for Soil Fertility and Agricultural Development; University of Guelph",
        "URI": "-",
        "DOI": "https://doi.org/10.2134/agronj1994.00021962008600060014x",
        "Extended description": "Determines soil temperature by layer. Calculates soil temperature for each soil layer and the surface litter layer using the Parton & Logan approach with damping depth modified by soil water content and soil thermal properties. Handles seasonal initialization and daily rate updates and calls SOILT to compute layer temperatures.",
        "Short description": "Determines soil temperature by layer",
        "Model version": "1.0"
    },
    "init": {
        "name": "STEMP",
        "firstLine": "SWI = SW",
        "lastLine": "END DO"
    },
    "process": {
        "name": "STEMP",
        "description": "Top-level subroutine that initializes and updates soil temperature (seasonal initialization and daily RATE) and calls SOILT to compute layer and surface soil temperatures.",

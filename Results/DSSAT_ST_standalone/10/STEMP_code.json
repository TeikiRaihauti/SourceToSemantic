                {
                    "name": "TAVG",
                    "value": "25.0"
                },
                {
                    "name": "TMAX",
                    "value": "30.0"
                },
                {
                    "name": "XLAT",
                    "value": "28.0"
                },
                {
                    "name": "TAV",
                    "value": "20.0"
                },
                {
                    "name": "TAMP",
                    "value": "10.0"

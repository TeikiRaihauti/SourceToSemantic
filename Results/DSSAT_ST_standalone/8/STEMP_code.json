    "functions": [
        {
            "name": "_fortran_nint",
            "description": "Nearest-integer function matching Fortran NINT behavior (halves away from zero)."
        },
        {
            "name": "_round_fortran",
            "description": "Fortran-style rounding to a specified number of decimals using NINT."
        },
        {
            "name": "SOILT",
            "description": "Compute soil temperature by layer and surface, updating 5-day temperature memory."
        }
    ],
    "tests": [
        {
            "name": "-",
            "description": "-",
            "inputs": [
                {
                    "name": "-",
                    "value": "-"
